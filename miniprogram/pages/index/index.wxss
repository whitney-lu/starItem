.container{
  width: 750rpx;
  height: 2000rpx;
  background: #cfcece;
}
.wrap{
  width: 100%;
  background: #cfcece;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 20rpx;
  box-sizing: border-box;
}
.itemOne{
  width: 340rpx;
  height: 590rpx;
  background: #ffffff;
  display: flex;
  flex-direction: column;
  margin-bottom: 40rpx;
}
.top{
  width: 100%;
  height: 60%;
  flex:1;
}
.top image{
  width: 100%;
  height: 100%;
} 
.bottom{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  display: flex;
}
.left{
  display: inline-block;
  width: 100rpx;
  height: 100rpx;
  padding: 10rpx 0 0 20rpx;
  box-sizing: border-box;
}
.right{
  display: inline-block;
  height: 100rpx;
  flex:1;
  font-size: 18rpx;
}
.left image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
}
